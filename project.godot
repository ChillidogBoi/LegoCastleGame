; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Kenney_Brick_Kit"
config/features=PackedStringArray("4.5", "GL Compatibility")
config/icon="res://icon.svg"

[autoload]

Settings="*res://code/menus/settings.gd"

[editor_plugins]

enabled=PackedStringArray("res://addons/maaacks_input_remapping/plugin.cfg")

[input]

esc={
"deadzone": 0.2,
"events": [Object(InputEventKey,"resource_local_to_scene":false,"resource_name":"","device":-1,"window_id":0,"alt_pressed":false,"shift_pressed":false,"ctrl_pressed":false,"meta_pressed":false,"pressed":false,"keycode":0,"physical_keycode":4194305,"key_label":0,"unicode":0,"location":0,"echo":false,"script":null)
]
}

[layer_names]

3d_physics/layer_1="ground"
3d_physics/layer_2="enemy_hurt"
3d_physics/layer_3="use"
3d_physics/layer_4="collection(touch)"

[maaacks_input_remapping]

disable_update_check=false
disable_install_wizard=true
copy_path="res://con_examp/"

[rendering]

renderer/rendering_method="gl_compatibility"
renderer/rendering_method.mobile="gl_compatibility"
